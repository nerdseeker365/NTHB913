<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
  <class name="com.nt.entity.Hospital"   table="HOSPITAL">
        <id name="hospitalId"  column="HOSPITALID"  type="int"  length="10">
           <generator class="increment"/>
        </id>  
       <property name="hospitalName"  column="HOSPITALNAME" type="string" length="20"/>
       <property name="addrs"  column="ADDRS"  type="string" length="20" />
       <property name="contactNo"  column="CONTACTNO"  type="long"  length="10" />
       <property name="capacity"  column="CAPACITY"  type="int"  length="5" />
  </class>

</hibernate-mapping>	